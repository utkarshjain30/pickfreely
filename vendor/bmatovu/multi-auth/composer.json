{
  "name": "bmatovu/multi-auth",
  "description": "Laravel Multi Authentication",
  "homepage": "https://github.com/mtvbrianking/multi-auth",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "package",
    "multi",
    "authentication"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/auth": "~5.7.0",
    "illuminate/console": "~5.7.0",
    "illuminate/notifications": "~5.7.0",
    "illuminate/routing": "~5.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "3.7.*"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\MultiAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\MultiAuth\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\MultiAuth\\MultiAuthServiceProvider"
      ]
    }
  }
}
